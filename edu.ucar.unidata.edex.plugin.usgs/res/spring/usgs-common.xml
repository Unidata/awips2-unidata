<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd  
  http://camel.apache.org/schema/spring http://camel.apache.org/schema/spring/camel-spring.xsd">

	<bean id="usgsPluginName" class="java.lang.String">
		<constructor-arg type="java.lang.String" value="usgs" />
	</bean>

	<bean id="usgsProperties" class="com.raytheon.uf.common.dataplugin.PluginProperties">
		<property name="pluginName" ref="usgsPluginName" />
		<property name="pluginFQN" value="edu.ucar.unidata.common.dataplugin.usgs" />
		<property name="record" value="edu.ucar.unidata.common.dataplugin.usgs.USGSRecord" />
	</bean>
	
	<bean id="usgsRegistered" factory-bean="pluginRegistry" factory-method="register">
		<constructor-arg ref="usgsPluginName"/>
		<constructor-arg ref="usgsProperties"/>
	</bean>

</beans>
